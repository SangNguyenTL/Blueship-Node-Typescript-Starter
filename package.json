{
  "name": "ogatlane-article",
  "version": "1.0.0",
  "description": "Ogatlane-Article",
  "private": true,
  "author": "Ogatlane",
  "scripts": {
    "start": "npm run serve",
    "build": "npm run lint && npm run copy-static-assets && npm run build-ts",
    "build:prod": "npm run lint && npm run copy-static-assets && npm run build-ts:prod",
    "serve": "ts-node --files src/server.ts",
    "watch-node": "nodemon src/server.ts",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-node\"",
    "test": "jest --forceExit --coverage --verbose --runInBand --detectOpenHandles",
    "watch-test": "npm run test -- --watchAll",
    "build-ts": "tsc",
    "build-ts:prod": "tsc -p tsconfig.prod.json",
    "watch-ts": "tsc -w",
    "lint": "tslint 'src/**/*.ts'",
    "prettier": "prettier --write \"src/**/*.ts\"",
    "copy-static-assets": "ts-node copyStaticAssets.ts",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect src/server.ts",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "dependencies": {
    "async": "^3.2.0",
    "bcrypt": "^4.0.1",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "colors": "^1.4.0",
    "compression": "^1.7.4",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "ejs": "^2.6.2",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "express-promise-router": "^3.0.3",
    "express-routemap": "^1.6.0",
    "express-session": "^1.15.6",
    "express-validator": "^6.1.1",
    "helmet": "^3.22.0",
    "is-promise": "^2.1.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mime-types": "^2.1.24",
    "module-alias": "^2.2.0",
    "moment": "^2.24.0",
    "mongoose": "^5.9.7",
    "mongoose-auto-increment-reworked": "^1.2.1",
    "mongoose-autopopulate": "^0.12.1",
    "mongoose-beautiful-unique-validation": "^7.1.1",
    "mongoose-validator": "^2.1.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "nocache": "^2.1.0",
    "node-schedule": "^1.3.2",
    "nodemailer": "^6.4.6",
    "omit.js": "^1.0.2",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "shelljs": "^0.8.3",
    "umi-request": "^1.2.19",
    "uuid": "^3.3.2",
    "validator": "^13.0.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/async": "^3.2.0",
    "@types/bcrypt": "^3.0.0",
    "@types/bluebird": "^3.5.30",
    "@types/body-parser": "^1.19.0",
    "@types/compression": "^0.0.36",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^6.1.1",
    "@types/ejs": "^2.6.3",
    "@types/errorhandler": "^1.5.0",
    "@types/express": "^4.17.4",
    "@types/express-routemap": "^1.1.1",
    "@types/express-serve-static-core": "^4.17.8",
    "@types/express-session": "^1.17.0",
    "@types/helmet": "^0.0.45",
    "@types/is-promise": "^2.1.0",
    "@types/jest": "^24.0.14",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/lodash": "^4.14.134",
    "@types/mailgen": "^2.0.2",
    "@types/mime-types": "^2.1.0",
    "@types/module-alias": "^2.0.0",
    "@types/mongoose": "^5.7.8",
    "@types/morgan": "^1.7.36",
    "@types/multer": "^1.4.3",
    "@types/node": "^12.7.2",
    "@types/node-schedule": "^1.3.0",
